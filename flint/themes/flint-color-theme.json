{
  "name": "Flint Theme",
  "type": "dark",
  "tokenColors": [
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#E35C26", // Softer fiery red-orange
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#FF8256" // Less saturated orange for operators
      }
    },
    {
      "scope": "keyword.return",
      "settings": {
        "foreground": "#FF5733", // Slightly softer red-orange for `return`
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.definition",
      "settings": {
        "foreground": "#C85A2E", // Muted reddish-orange for `def`, `data`
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.primitive",
      "settings": {
        "foreground": "#CC7433" // Softer copper-orange for primitive types
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#E87440", // Less intense orange for `data`, `func`
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#E55B3C", // Slightly subdued orange for function names
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#D87C30", // Warmer orange for struct/class names
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#DB7033", // Softer orange for `true`, `false`
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#E06645" // Mellowed tomato red for numbers
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#8A8A8A", // Slightly lighter gray for readability
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.quoted",
      "settings": {
        "foreground": "#FF9E7A" // Less saturated salmon for strings
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#C2573E" // Less intense burnt orange for variables
      }
    },
    {
      "scope": "identifier.field",
      "settings": {
        "foreground": "#BF593E" // Softer burnt orange for struct fields
      }
    },
    {
      "scope": "identifier.constant",
      "settings": {
        "foreground": "#B05035" // Warmer red-orange for uppercase constants
      }
    },
    {
      "scope": "operator",
      "settings": {
        "foreground": "#C27B50" // Muted brown-orange for operators
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#A5A5A5" // Slightly lighter gray for punctuation
      }
    }
  ]
}
