{
  "name": "Flint Theme",
  "type": "dark",
  "colors": {
    "editorGroup.border": "#4f5d64",
    "sideBar.background": "#1f1f28",
    "sideBarSectionHeader.background": "#1f1f28",
    "sideBarStickyScroll.background": "#292936",
    "sideBar.foreground": "#c0bab2",
    "activityBar.background": "#292936",
    "activityBar.inactiveForeground": "#c0bab2",
    "activityBar.foreground": "#c0af87",
    "statusBar.background": "#292936",
    "editor.background": "#292936",
    "window.activeBorder": "#2f343f",
    "window.inactiveBorder": "#4f5d64",
    "titleBar.activeBackground": "#292936",
    "titleBar.activeForeground": "#c0bab2",
    "tab.inactiveBackground": "#1f1f28",
    "tab.inactiveForeground": "#c0bab2",
    "tab.activeForeground": "#c0af87",
  },
  "tokenColors": [
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#a292a3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.exception",
      "settings": {
        "foreground": "#c4746e"
      }
    },
    {
      "scope": "keyword.preproc",
      "settings": {
        "foreground": "#c4746e"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#A5A5A5" // Light grey for operators
      }
    },
    {
      "scope": "keyword.declaration",
      "settings": {
        "foreground": "#a292a3"
      }
    },
    {
      "scope": "keyword.other.documentation",
      "settings": {
        "foreground": "#658594",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#658594",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#c4746e"
      }
    },
    {
      "scope": "storage.type", // Primitives
      "settings": {
        "foreground": "#8ea49e"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#8ba5b4"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#8ea49e"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#b6927b"
      }
    },
    {
      "scope": "constant.other",
      "settings": {
        "foreground": "#b6927b"
      }
    },
    {
      "scope": "constant.other.tuple-access",
      "settings": {
        "foreground": "#c0af87"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#a292a3"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#727169",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.quoted",
      "settings": {
        "foreground": "#8a9a7b"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#c0af87"
      }
    },
    {
      "scope": "identifier.field",
      "settings": {
        "foreground": "#BF593E" // Softer burnt orange for struct fields
      }
    },
    {
      "scope": "identifier.constant",
      "settings": {
        "foreground": "#B05035" // Warmer red-orange for uppercase constants
      }
    },
    {
      "scope": "operator",
      "settings": {
        "foreground": "#C27B50" // Muted brown-orange for operators
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#A5A5A5" // Slightly lighter gray for punctuation
      }
    }
  ]
}
